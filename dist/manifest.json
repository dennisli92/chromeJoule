{
  "manifest_version": 2,

  "name": "Chrome Joule",
  "description": "Chrome app to control Joule",
  "version": "1.0",

  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["js/vendor.js", "js/content_script.js"]
      }
  ],

  "background": {
    "scripts": ["js/vendor.js", "js/background.js"]
  },

  "permissions": ["<all_urls>"]
}